alignment bottom_right
background no
cpu_avg_samples 2
default_color saddle brown
draw_borders no
draw_outline no
draw_shades no
use_xft yes
xftfont DejaVu Sans Mono:size=8
gap_x 80
gap_y 240
minimum_size 100 100
net_avg_samples 2
no_buffers yes
out_to_console no
out_to_stderr no
extra_newline no
own_window yes
own_window_class Conky
own_window_type normal
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager
own_window_transparent yes
stippled_borders 0
update_interval 1.0
uppercase no
use_spacer none
double_buffer yes

TEXT
CPU 1    ${cpu cpu0}%
${cpubar cpu0 10,75}

CPU 2    ${cpu cpu1}%
${cpubar cpu1 10,75}

CPU 3    ${cpu cpu2}%
${cpubar cpu2 10,75}

CPU 4    ${cpu cpu3}%
${cpubar cpu3 10,75}
